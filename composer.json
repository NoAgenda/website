{
    "name": "codedmonkey/noagenda",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "babdev/pagerfanta-bundle": "^2.8",
        "benlipp/srt-parser": "^1.1",
        "easycorp/easyadmin-bundle": "^2.1",
        "laminas/laminas-feed": "^2.10",
        "liip/imagine-bundle": "^2.1",
        "phergie/phergie-irc-client-react": "^3.5",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-pack": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/flex": "^1.0",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/maker-bundle": "^1.14",
        "symfony/messenger": "^5.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-php80": "*",
        "symfony/process": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/web-link": "^5.2",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^5.2",
        "vipnytt/opmlparser": "dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-pack": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2"
        }
    }
}
