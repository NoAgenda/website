{
    "name": "noagenda/website",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "babdev/pagerfanta-bundle": "^3.6",
        "benlipp/srt-parser": "^1.1",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.9",
        "dragonmantank/cron-expression": "^3.3",
        "easycorp/easyadmin-bundle": "4.2.*",
        "laminas/laminas-feed": "^2.10",
        "liip/imagine-bundle": "^2.1",
        "minishlink/web-push": "^7.0",
        "pagerfanta/doctrine-orm-adapter": "^3.6",
        "pagerfanta/twig": "^3.6",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.2",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/doctrine-messenger": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^2.4",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/messenger": "^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "^6.0",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/scheduler": "6.4.*",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/telegram-notifier": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^6.0",
        "vipnytt/opmlparser": "dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "assets:build": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": false,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false,
            "require": "6.4.*"
        }
    }
}
