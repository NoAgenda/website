{
    "name": "codedmonkey/noagenda-experience",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "repositories": [
        {"type": "github", "url": "git@github.com:codedmonkey/onlinq-monitoring.git"},
        {"type": "github", "url": "git@github.com:codedmonkey/onlinq-monitoring-bundle.git"}
    ],
    "require": {
        "php": "^7.1.3",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "colorfield/mastodon-api": "dev-master",
        "easycorp/easyadmin-bundle": "^2.1",
        "liip/imagine-bundle": "^2.1",
        "neitanod/forceutf8": "^2.0",
        "onlinq/monitoring-bundle": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-pack": "^1.2",
        "rtheunissen/oauth2-reddit": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-link": "^4.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^4.4",
        "white-october/pagerfanta-bundle": "^1.2",
        "zendframework/zend-feed": "^2.10"
    },
    "require-dev": {
        "ext-json": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "monitor:status --quiet --log": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
