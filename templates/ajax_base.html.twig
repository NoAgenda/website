{% set page_title = page_title is not defined ? 'No Agenda' : "#{page_title} - No Agenda" %}
{% set body = block('body') %}
{% set metatags = block('metatags') %}
{% set request_path = path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) %}

{% set output = {
  'path': request_path,
  'title': page_title,
  'meta': metatags,
  'contents': body,
  'authenticated': authenticated,
} %}

{{ output|json_encode|raw }}
