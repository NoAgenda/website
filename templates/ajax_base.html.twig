{% set page_title = page_title is not defined ? 'No Agenda' : "#{page_title} - No Agenda" %}
{% set body = block('body') %}
{% set metatags = block('metatags') %}
{% set request_path = ajaxRequestPath() %}

{% set output = {
  'path': request_path,
  'fragment': app.request.query.get('_fragment'),
  'title': page_title,
  'meta': metatags,
  'contents': body,
  'authenticated': authenticated,
} %}

{{ output|json_encode|raw }}
